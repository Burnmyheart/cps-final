#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/projects/cps/cps-final/node_modules/.pnpm/jpegtran-bin@7.0.0/node_modules/jpegtran-bin/node_modules:/mnt/d/projects/cps/cps-final/node_modules/.pnpm/jpegtran-bin@7.0.0/node_modules:/mnt/d/projects/cps/cps-final/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/projects/cps/cps-final/node_modules/.pnpm/jpegtran-bin@7.0.0/node_modules/jpegtran-bin/node_modules:/mnt/d/projects/cps/cps-final/node_modules/.pnpm/jpegtran-bin@7.0.0/node_modules:/mnt/d/projects/cps/cps-final/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jpegtran-bin@7.0.0/node_modules/jpegtran-bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../jpegtran-bin@7.0.0/node_modules/jpegtran-bin/cli.js" "$@"
fi
